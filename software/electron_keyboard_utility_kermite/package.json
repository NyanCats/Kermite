{
  "name": "electron-keyboard-utility-astelia",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "^13.0.0"
  },
  "scripts": {
    "postinstall": "electron-rebuild -o node-hid",
    "fixlib": "electron-rebuild -o node-hid -f",
    "start": "cross-env NODE_ENV=production node ./boot.js",
    "start:dev": "cross-env NODE_ENV=development node ./boot.js bundle start",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint src/ --ext .ts,.tsx",
    "lint:fix": "npx eslint src/ --ext .ts,.tsx --fix",
    "clean": "rm -rf dist .cache"
  },
  "dependencies": {
    "@types/node-hid": "^1.2.0",
    "@types/parcel-bundler": "^1.12.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "child_process": "^1.0.2",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "dyo": "^2.0.4",
    "electron": "^8.2.5",
    "electron-rebuild": "^1.10.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "goober": "^1.8.0",
    "node-hid": "^1.2.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
