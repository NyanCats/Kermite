{
  "env": {
    "myCompilerBaseDirectory__windows_example": "C:/tools/avr8-gnu-toolchain-win32_x86",
    "myCompilerExecutablePath__windows_example": "C:/tools/avr8-gnu-toolchain-win32_x86/bin/avr-gcc.exe",
    "myCompilerBaseDirectory": "/usr/local/Cellar/avr-gcc/9.3.0",
    "myCompilerExecutablePath": "/usr/local/bin/avr-gcc"
  },
  "configurations": [
    {
      "name": "Mac",
      "includePath": [
        "${myCompilerBaseDirectory}/avr/include",
        "src/modules/km0/common",
        "src/modules/km0/device_io",
        "src/modules/km0/keyboard",
        "src/projects/dev/dummy_header_provider_project"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "__AVR_ATmega32U4__",
        "EXTR_PROJECT_RELEASE_BUILD_REVISION=0",
        "EXTR_IS_RESOURCE_ORIGIN_ONLINE=0",
        "F_CPU=8000000",
        "TARGET_MCU_ATMEGA"
      ],
      "compilerPath": "${myCompilerExecutablePath} -mmcu=atmega32u4",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "clang-x64",
      "browse": {
        "path": ["src", "${myCompilerBaseDirectory}/avr/include"],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
  ],
  "version": 4
}
