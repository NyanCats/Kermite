{
  "env": {
    "myCompilerBaseDirectory__windows_example": "C:/tools/avr8-gnu-toolchain-win32_x86",
    "myCompilerExecutablePath__windows_example": "C:/tools/avr8-gnu-toolchain-win32_x86/bin/avr-gcc.exe",
    "myAvrCompilerBaseDirectory": "/usr/local/Cellar/avr-gcc/9.3.0",
    "myAvrCompilerExecutablePath": "/usr/local/bin/avr-gcc",
    "myCompilerExecutablePath": "/usr/local/bin/arm-none-eabi-gcc",
    "picoSdkRoot": "./deps/rp2040/external/pico_sdk",
    "picoLocalDir": "./deps/rp2040/local"
  },
  "configurations": [
    {
      "name": "Mac",
      "includePath": [
        "src/modules/km0/common",
        "src/modules/km0/device_io",
        "src/modules/km0/keyboard",
        "src/projects/dev/dummy_header_provider_project",
        "${myAvrCompilerBaseDirectory}/avr/include",
        "${picoLocalDir}/include",
        // "${picoLocalDir}/include/boards",
        "${picoSdkRoot}/src/rp2040/hardware_regs/include",
        "${picoSdkRoot}/src/rp2040/hardware_structs/include",
        "${picoSdkRoot}/src/common/pico_base/include",
        "${picoSdkRoot}/src/common/pico_stdlib/include",
        "${picoSdkRoot}/src/common/pico_time/include",
        "${picoSdkRoot}/src/common/pico_sync/include",
        "${picoSdkRoot}/src/common/pico_util/include",
        "${picoSdkRoot}/src/common/pico_bit_ops/include",
        "${picoSdkRoot}/src/common/pico_divider/include",
        "${picoSdkRoot}/src/common/pico_binary_info/include",
        "${picoSdkRoot}/src/rp2_common/pico_platform/include",
        "${picoSdkRoot}/src/rp2_common/pico_runtime/include",
        "${picoSdkRoot}/src/rp2_common/hardware_gpio/include",
        "${picoSdkRoot}/src/rp2_common/hardware_flash/include",
        "${picoSdkRoot}/src/rp2_common/hardware_base/include",
        "${picoSdkRoot}/src/rp2_common/hardware_claim/include",
        "${picoSdkRoot}/src/rp2_common/hardware_sync/include",
        "${picoSdkRoot}/src/rp2_common/hardware_uart/include",
        "${picoSdkRoot}/src/rp2_common/hardware_divider/include",
        "${picoSdkRoot}/src/rp2_common/hardware_timer/include",
        "${picoSdkRoot}/src/rp2_common/hardware_clocks/include",
        "${picoSdkRoot}/src/rp2_common/hardware_resets/include",
        "${picoSdkRoot}/src/rp2_common/hardware_watchdog/include",
        "${picoSdkRoot}/src/rp2_common/hardware_xosc/include",
        "${picoSdkRoot}/src/rp2_common/hardware_pll/include",
        "${picoSdkRoot}/src/rp2_common/hardware_vreg/include",
        "${picoSdkRoot}/src/rp2_common/hardware_irq/include",
        "${picoSdkRoot}/src/rp2_common/hardware_pio/include",
        "${picoSdkRoot}/src/rp2_common/pico_printf/include",
        "${picoSdkRoot}/src/rp2_common/pico_bootrom/include",
        "${picoSdkRoot}/src/rp2_common/pico_double/include",
        "${picoSdkRoot}/src/rp2_common/pico_int64_ops/include",
        "${picoSdkRoot}/src/rp2_common/pico_float/include",
        "${picoSdkRoot}/src/rp2_common/pico_malloc/include",
        "${picoSdkRoot}/src/rp2_common/pico_stdio/include",
        "${picoSdkRoot}/src/rp2_common/pico_stdio_uart/include",
        "${picoSdkRoot}/src/rp2_common/pico_multicore/include",
        "${picoSdkRoot}/lib/tinyusb/src",
        "${picoSdkRoot}/lib/tinyusb/src/common",
        "${picoSdkRoot}/lib/tinyusb/hw",
        "${picoSdkRoot}/src/rp2_common/pico_fix/rp2040_usb_device_enumeration/include"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "__AVR_ATmega32U4__",
        "EXTR_PROJECT_RELEASE_BUILD_REVISION=0",
        "EXTR_IS_RESOURCE_ORIGIN_ONLINE=0",
        "EXTR_VARIATION_NAME=\"default\"",
        "F_CPU=8000000",
        "TARGET_MCU_ATMEGA",
        "TARGET_MCU_RP2040",
        "CFG_TUSB_MCU=OPT_MCU_RP2040",
        "SINGLEWIRE_RP2040_PIN_SIGNAL=GP0"
      ],
      // "compilerPath": "${myAvrCompilerExecutablePath} -mmcu=atmega32u4",
      "compilerPath": "${myCompilerExecutablePath}",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-arm",
      "browse": {
        "path": [
          "src",
          "${myAvrCompilerBaseDirectory}/avr/include",
          "${picoSdkRoot}"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
  ],
  "version": 4
}
